# Neon PostgreSQL Configuration
# Get connection string from https://console.neon.tech

DATABASE_URL=postgresql://user:password@host/database_name

# JWT Secret (change in production!)
SECRET_KEY=your-secret-key-change-this-in-production

# Algorithm
ALGORITHM=HS256

# Access Token Expiration (in minutes)
ACCESS_TOKEN_EXPIRE_MINUTES=480

# Frontend CORS origins
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8000","https://yourdomain.com"]

# ML Models Path
MODELS_PATH=../ckd_project/models/saved_models
